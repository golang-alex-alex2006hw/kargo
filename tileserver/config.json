      "style": "osm-bright/style.json"
    }
  },
  "data": {
    "DATASET_1": {
      "mbtiles": "dataset_1.mbtiles"
    },
    "DATASET_2": {
      "mbtilee": "dataset_2.mbtiles"
    }
  }
